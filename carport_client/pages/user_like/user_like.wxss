/* pages/user_like/user_like.wxss */

.carport_list{
  width: 98%;
  height: 60%;
  order: 3;
  margin-left: 30rpx;
  margin-top: 10%;
}
.carport_list .msg{
  margin-top: 5px;
}

.container_carport_msg{
  width: 95%;
  height: 200rpx;
  background-color: #f5f2f2;
  border-width: 1rpx;
  border-color: #f5f2f2;
  border-style: solid;
  display: flex;
  flex-direction: row;
  border-radius: 7%;
  box-shadow:0px 2px 5px 5px #e6e4e4;
  margin-top: 10rpx;
}
.container_carport_msg .carport_img{
  width: 250rpx;
  height: 200rpx;
  border-radius: 7%;
}
.msg{
  margin-left: 10px;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  font-size: 14px;
}
.msg .title{
  font-size: 15px;
  font-weight: 600;
}
.address{
  font-size: 13;
  font-weight: 500;
  color: #5c5757;
}
.type_price{
  display: flex;
  flex-direction: row;
}
.type_price .price{
  margin-left: 30px;
}
.type_price .price text{
  color: red;
}
.test_buttom{
  margin: 100px;
}